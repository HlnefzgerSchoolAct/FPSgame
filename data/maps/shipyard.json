{
  "$schema": "map-schema",
  "version": "1.0.0",
  "map_id": "shipyard",
  "name": "Shipyard",
  "description": "Asymmetric dockside map with containers, cranes, and ships. Multiple vertical levels and varied engagement ranges.",
  "enabled": true,
  "environment": "coastal",
  "theme": "industrial_port",
  "time_of_day": "dawn",
  "weather": "foggy",
  "dimensions": {
    "length": 90,
    "width": 75,
    "height_min": 0,
    "height_max": 20,
    "units": "meters"
  },
  "layout": {
    "design_philosophy": "three_lane_asymmetric",
    "lanes": [
      {
        "id": "docks_lane",
        "name": "Docks",
        "type": "outdoor_elevated",
        "width": 18,
        "length": 70,
        "primary_engagement_range": "medium_long",
        "cover_density": "low",
        "verticality": "high",
        "description": "Elevated dock platforms with long sightlines and crane access. Favors ARs and snipers."
      },
      {
        "id": "container_lane",
        "name": "Container Yard",
        "type": "mixed",
        "width": 25,
        "length": 65,
        "primary_engagement_range": "close_medium",
        "cover_density": "very_high",
        "verticality": "medium",
        "description": "Dense container maze with multiple routes. CQB heaven for SMGs and shotguns."
      },
      {
        "id": "ship_lane",
        "name": "Ship Interior",
        "type": "indoor",
        "width": 15,
        "length": 50,
        "primary_engagement_range": "close",
        "cover_density": "medium",
        "verticality": "medium",
        "description": "Inside cargo ship with corridors and rooms. Tight combat favoring SMGs."
      }
    ],
    "connectors": [
      {
        "connects": ["docks_lane", "container_lane"],
        "position": "north",
        "type": "ramp",
        "width": 5
      },
      {
        "connects": ["container_lane", "ship_lane"],
        "position": "north",
        "type": "gangway",
        "width": 3
      },
      {
        "connects": ["docks_lane", "container_lane"],
        "position": "center",
        "type": "ladder",
        "width": 1
      },
      {
        "connects": ["container_lane", "ship_lane"],
        "position": "south",
        "type": "loading_ramp",
        "width": 6
      },
      {
        "connects": ["docks_lane", "ship_lane"],
        "position": "south",
        "type": "crane_platform",
        "width": 4
      }
    ]
  },
  "callouts": {
