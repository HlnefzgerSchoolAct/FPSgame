      "fog_density": "medium"
    },
    "audio_zones": {
      "reverb_areas": ["ship_cargo_hold", "warehouse"],
      "outdoor_areas": ["docks", "container_yard"],
      "metal_footsteps": ["ship_deck", "upper_docks", "crane"]
    },
    "weather_effects": {
      "fog": true,
      "rain": false,
      "visibility_reduction": 0.15
    },
    "destructible_elements": [
      { "type": "crates", "count": 8 },
      { "type": "barrels", "count": 6 }
    ],
